{
  "name": "google-oauth-backend",
  "version": "1.0.0",
  "description": "A secure Node.js/Express backend implementing Google OAuth 2.0 authentication with JWT token management and PostgreSQL database integration",
  "type": "commonjs",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/arpan-lol/Google-Oauth-JWT-Backend.git"
  },
  "author": "Arpan <arpantaneja14@gmail.com>",
  "license": "MIT",
  "keywords": [
    "oauth",
    "google-oauth",
    "jwt",
    "authentication",
    "express",
    "typescript",
    "prisma",
    "postgresql",
    "backend",
    "rest-api"
  ],
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "google-auth-library": "^10.4.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.11.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.13",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
